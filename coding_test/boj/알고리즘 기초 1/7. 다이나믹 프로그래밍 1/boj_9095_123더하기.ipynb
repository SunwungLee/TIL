   "source": [
    "cases = int(input())\n",
    "\n",
    "dp = [0]*12 # n은 11보다 작다고 정해짐\n",
    "dp[1] = 1\n",
    "dp[2] = 2\n",
    "dp[3] = 4\n",
    "\n",
    "for _ in range(cases):\n",
    "    n = int(input())\n",
    "    for i in range(4, n+1):\n",
    "        dp[i] = dp[i-1] + dp[i-2] + dp[i-3]\n",
    "    print(dp[n])"
   ]
