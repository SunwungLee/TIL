   "source": [
    "end = int(input())\n",
    "\n",
    "n = len(str(end))\n",
    "\n",
    "def counting(n):\n",
    "    if n == 0:\n",
    "        return 0\n",
    "    if n == 1:\n",
    "        return 9\n",
    "    return (9*(10**(n-1))*n) + counting(n-1)\n",
    "\n",
    "cnt = counting(n-1) + ((end - 10**(n-1))*(n)) + n\n",
    "    \n",
    "print(cnt)"
   ]
