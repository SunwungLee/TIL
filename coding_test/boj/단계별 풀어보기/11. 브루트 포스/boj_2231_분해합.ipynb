    "# 245 = 245+2+4+5\n",
    "# \n",
    "n = int(input())\n",
    "\n",
    "for i in range(1, n+1):\n",
    "    res = i\n",
    "    for j in str(i):\n",
    "        res += int(j)\n",
    "    if res == n:\n",
    "        print(i)\n",
    "        break\n",
    "    if i == n:\n",
    "        print(0)"
